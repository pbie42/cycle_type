{
    "compileOnSave": false,
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "lib": [
            "es2015",
            "es2016",
						"es2017",
            "DOM"
        ],
        "target": "es2017",
        "module": "es2015",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "allowJs": true
    }
}
